

    <% for(var i = 0; i < users.length; i++) { %>
        <tr>
            <td><%= i + 1 %></td>
            <td><%= users[i].firstname %></td>
            <td><%= users[i].lastname %></td>
            <td><%= users[i].email %></td>
            <td><%= users[i].gender %></td>
        
            <% if (users[i].status == 'Active') { %>
                <td style='color:green;'><%= users[i].status  %></td>
                <% }else{ %>
                <td style='color:red;'><%= users[i].status %></td>
                <% } %>
        
        
            <td>
                <a href="/update-user?id=<%= users[i].id%>" class="btn border-shadow update">
                    <span ><i class="fa fa-pencil fa_custom"></i></span>
                </a>
                <a class="btn border-shadow delete"  data-id=<%= users[i].id%> >
                   <span> <i class="fa fa-trash-o fa_custom"></i></span>
                </a>
            </td>
        </tr>
        <% } %>
       